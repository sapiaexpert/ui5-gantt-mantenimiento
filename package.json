{
  "name": "ui5-gantt-maintenance",
  "version": "1.0.0",
  "description": "SAP Maintenance Order Gantt Chart Application",
  "author": "sapiaexpert",
  "license": "MIT",
  "scripts": {
    "start": "ui5 serve --port 8080 --open index.html",
    "start:mock": "ui5 serve --port 8080 --open index.html",
    "start:prod": "ui5 serve --port 8080 --open index-production.html",
    "build": "ui5 build --clean-dest --dest dist",
    "build:cf": "ui5 build preload --clean-dest --dest dist --include-task=generateManifestBundle generateCachebusterInfo",
    "build:mta": "mbt build",
    "deploy": "npm run build:mta && cf deploy mta_archives/ui5-gantt-maintenance_1.0.0.mtar",
    "deploy:dev": "npm run build:mta && cf deploy mta_archives/ui5-gantt-maintenance_1.0.0.mtar -f",
    "undeploy": "cf undeploy ui5-gantt-maintenance --delete-services --delete-service-keys",
    "clean": "rimraf dist mta_archives .che node_modules",
    "lint": "eslint webapp/",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1.14.0",
    "@ui5/cli": "^3.11.11",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "ui5-tooling-transpile": "^3.9.2"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling"
    ]
  },
  "sapux": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sapiaexpert/ui5-gantt-mantenimiento.git"
  },
  "keywords": [
    "SAPUI5",
    "Gantt",
    "Maintenance",
    "SAP S/4HANA",
    "OData V4",
    "BTP"
  ]
}
